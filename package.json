{
  "name": "typescript-utility",
  "version": "0.0.0-semantically-released",
  "description": "just sample library for custom typescript utility",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js",
    "lint": "eslint src",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "coverage": "cross-env NODE_ENV=test nyc mocha src/index.test.js",
    "validate": "npm-run-all --parallel coverage lint build",
    "release": "semantic-release"
  },
  "keywords": [
    "library"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkaushikl/typescript-utility.git"
  },
  "author": "Kaushik Dhameliya <dkaushikl1610@gmail.com> (http://dkaushikl.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkaushikl/typescript-utility/issues"
  },
  "homepage": "https://github.com/dkaushikl/typescript-utility#readme",
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.2.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "3.2.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "1.3.2",
    "json-loader": "0.5.4",
    "mocha": "^8.1.1",
    "npm-run-all": "2.3.0",
    "nyc": "^15.1.0",
    "rimraf": "2.5.4",
    "semantic-release": "^17.1.1",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "function": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ],
    "sourceMap": false,
    "instrument": false,
    "require": [
      "@babel/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "validate-commit-msg"
    }
  }
}
